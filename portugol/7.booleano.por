//7 - Faça um algoritmo que leia dois valores booleanos (lógicos) e determine se ambos são VERDADEIRO ou FALSO.
programa {
  funcao inicio() {
    logico valor1
    logico valor2

    escreva("Valor 1 - VERDADEIRO ou FALSO?\n")
    leia(valor1)
    escreva("Valor 2 - VERDADEIRO ou FALSO?\n")
    leia(valor2)
    escreva("Valor 1 - "+valor1+"\nValor 2 - "+valor2+"\n")
  }
}
